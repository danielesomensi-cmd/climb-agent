{
  "id": "finger_max_strength",
  "name": "Finger Strength – Max Hangs (Module)",
  "version": "1.2",
  "category": "strength_module",
  "sports": ["climbing"],
  "disciplines": ["boulder", "lead", "mixed"],

  "compatible_slots": ["medium", "long"],
  "requires_environment": {
    "equipment_any_of": ["hangboard", "fingerboard"]
  },

  "goals": ["increase_max_finger_force"],

  "stress_tags": {
    "fingers": "high",
    "cns": "medium",
    "elbow": "medium",
    "skin": "medium"
  },

  "scheduling_rules": {
    "max_per_week": 2,
    "min_rest_days_after": 1,
    "avoid_if_yesterday_stress_tags_any_of": ["fingers:high"]
  },

  "blocks": [
    {
      "block_id": "warmup_specific",
      "type": "warmup_specific",
      "duration_min": 12,
      "notes": "Specific hangboard warm-up: easy hangs + recruitment + 2–3 progressive sets."
    },
    {
      "block_id": "main",
      "type": "main_set",
      "protocol_family": "max_hangs",

      "exercise_selection": {
        "library": "catalog/exercises/v1/exercises.json",
        "match_all_tags": ["finger_strength", "max_hang", "main_exercise"],
        "prefer_exercise_ids": ["max_hang_7s", "max_hang_5s", "max_hang_ladder"],
        "diversity_rules": {
          "avoid_same_exercise_within_days": 7
        }
      },

      "prescription": {
        "sets_range": [5, 8],
        "hang_seconds_range": [5, 10],
        "rest_seconds_range": [150, 240],
        "intensity_pct_of_total_load_range": [0.85, 0.95]
      },

      "variables": {
        "grip_options": ["half_crimp", "open_hand"],
        "preferred_grip": "half_crimp",

        "edge_mm_options": [8, 10, 12, 15, 20],
        "preferred_edge_mm": 20,

        "load_method": ["added_weight", "assistance"],
        "preferred_load_method": "added_weight"
      },

      "intensity_reference": {
        "reference_test_id": "max_hang_5s_total_load",
        "definition": "total_load = bodyweight + external_load",
        "same_setup_required": true,
        "setup_fields": ["edge_mm", "grip", "load_method"],
        "if_missing": {
          "action": "estimate_then_schedule_test",
          "estimation_confidence": "low",
          "schedule_test_within_days": 14
        }
      },

      "safety_notes": [
        "Stop if sharp pain in fingers or elbow.",
        "Avoid full crimp unless explicitly allowed.",
        "If intensity feels inconsistent, downgrade load or schedule a proper test."
      ]
    },
    {
      "block_id": "cooldown_prehab_light",
      "type": "cooldown_prehab",
      "duration_min": 12,
      "exercise_selection": {
        "library": "catalog/exercises/v1/exercises.json",
        "match_any_tags": ["elbow_prehab", "scapular_strength"],
        "prefer_exercise_ids": [
          "elbow_wrist_extensor_eccentric",
          "elbow_pronation_supination",
          "scapular_pullups"
        ],
        "diversity_rules": {
          "avoid_same_exercise_within_days": 2
        }
      },
      "notes": "Light prehab after main set: elbow/scapular work + forearm flush. Keep it easy."
    }
  ],

  "module_notes": "Standalone module: specific warm-up + main finger strength + light prehab/cooldown. Do not include core/cardio/aesthetics here; those are separate modules combined at session/day level."
}

{
  "id": "finger_max_strength",
  "name": "Finger Strength – Max Hangs",
  "version": "1.1",
  "category": "strength",
  "sports": ["climbing"],
  "disciplines": ["boulder", "lead", "mixed"],

  "compatible_slots": ["medium", "long"],
  "requires_environment": {
    "equipment_any_of": ["hangboard", "fingerboard"]
  },

  "goals": ["increase_max_finger_force"],

  "stress_tags": {
    "fingers": "high",
    "cns": "medium",
    "elbow": "medium",
    "skin": "medium"
  },

  "scheduling_rules": {
    "max_per_week": 2,
    "min_rest_days_after": 1,
    "avoid_if_yesterday_stress_tags_any_of": ["fingers:high"]
  },

  "blocks": [
    {
      "block_id": "warmup",
      "type": "warmup",
      "duration_min": 12,
      "notes": "Progressive warm-up: easy hangs, recruitment pulls, 2–3 progressive sets."
    },
    {
      "block_id": "main",
      "type": "main_set",
      "protocol_family": "max_hangs",

      "prescription": {
        "sets_range": [5, 8],
        "hang_seconds_range": [5, 10],
        "rest_seconds_range": [150, 240],
        "intensity_pct_of_total_load_range": [0.85, 0.95]
      },
"exercise_selection": {
  "library": "catalog/exercises/v1/exercises.json",
  "match_all_tags": ["finger_strength", "max_hang", "main_exercise"],
  "prefer_exercise_ids": ["max_hang_7s", "max_hang_5s", "max_hang_ladder"],
  "diversity_rules": {
    "avoid_same_exercise_within_days": 7
  }
},
{
  "block_id": "core",
  "type": "optional_accessory",
  "duration_min": 10,
  "exercise_selection": {
    "library": "catalog/exercises/v1/exercises.json",
    "match_all_tags": ["core"],
    "prefer_exercise_ids": ["core_hollow_hold", "core_l_sit"],
    "diversity_rules": {
      "avoid_same_exercise_within_days": 3
    }
  },
  "notes": "Include core if time allows and it does not reduce main set quality."
},




      "variables": {
        "grip_options": ["half_crimp", "open_hand"],
        "preferred_grip": "half_crimp",

        "edge_mm_options": [8, 10, 12, 15, 20],
        "preferred_edge_mm": 20,

        "load_method": ["added_weight", "assistance"],
        "preferred_load_method": "added_weight"
      },

      "intensity_reference": {
        "reference_test_id": "max_hang_5s_total_load",
        "definition": "total_load = bodyweight + external_load",
        "same_setup_required": true,
        "setup_fields": ["edge_mm", "grip", "load_method"],
        "if_missing": {
          "action": "estimate_then_schedule_test",
          "estimation_confidence": "low",
          "schedule_test_within_days": 14
        }
      },

      "safety_notes": [
        "Stop immediately if sharp pain in fingers or elbow.",
        "Avoid full crimp unless explicitly allowed.",
        "If estimated load feels inconsistent, downgrade intensity or schedule a test."
      ]
    },
    {
      "block_id": "accessory",
      "type": "accessory",
      "duration_min": 12,
      "menu": [
        { "exercise_tag": "elbow_prehab", "priority": "high" },
        { "exercise_tag": "scapular_strength", "priority": "medium" }
      ]
    },
    {
      "block_id": "cooldown",
      "type": "cooldown",
      "duration_min": 5,
      "notes": "Light mobility + easy forearm flush."
    }
  ],

  "default_user_overrides": {
    "notes": "Default reference edge is 20mm. Other edges are variations, not baseline metrics."
  }
}

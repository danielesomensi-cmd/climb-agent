{
  "id": "general_warmup",
  "name": "General Warm-up (Module)",
  "version": "v1",
  "category": "warmup_module",
  "sport": "climbing",
  "discipline": ["boulder", "sport", "mixed"],

  "compatible_slots": ["short", "medium", "long"],
  "required_environment": { "any_of": ["home", "gym"] },

  "goals": ["increase_body_temperature", "improve_joint_readiness", "activate_shoulders"],

  "stress_tags": {
    "fingers": "none",
    "cns": "low",
    "elbow": "low",
    "skin": "none"
  },

  "schedule_rules": {
    "max_per_day": 2,
    "notes": "Use before primary modules. Skip if already warm (easy climbing/jogging)."
  },

  "blocks": [
    {
      "block_id": "pulse_raise",
      "type": "warmup_general",
      "selection": {
        "primary": {
          "filters": {
            "required_tags_all": ["stim:warmup_general"],
            "exclude_tags_any": ["stim:finger_max_strength"]
          }
        },
        "fallbacks": [
          {
            "filters": {
              "required_tags_any": ["stim:mobility", "stim:prehab_shoulder", "stim:scapular_strength"],
              "exclude_tags_any": ["stim:finger_max_strength"]
            }
          }
        ]
      },
      "prescription": {
        "duration_min_range": [4, 8],
        "notes": [
          "Goal: raise body temperature and breathing rate slightly. Keep easy."
        ]
      }
    },

    {
      "block_id": "mobility",
      "type": "mobility",
      "selection": {
        "primary": {
          "filters": {
            "required_tags_all": ["stim:mobility"],
            "exclude_tags_any": ["stim:finger_max_strength"]
          }
        },
        "fallbacks": [
          {
            "filters": {
              "required_tags_any": ["stim:prehab_shoulder", "stim:prehab_elbow"],
              "exclude_tags_any": ["stim:finger_max_strength"]
            }
          }
        ]
      },
      "prescription": {
        "duration_min_range": [4, 8],
        "notes": [
          "Dynamic mobility only. No long static stretches."
        ]
      }
    },

    {
      "block_id": "upper_activation",
      "type": "activation",
      "selection": {
        "primary": {
          "filters": {
            "required_tags_any": ["stim:scapular_strength", "stim:prehab_shoulder"],
            "exclude_tags_any": ["stim:finger_max_strength"]
          }
        },
        "fallbacks": [
          {
            "filters": {
              "required_tags_all": ["stim:prehab_elbow"],
              "exclude_tags_any": ["stim:finger_max_strength"]
            }
          }
        ]
      },
      "prescription": {
        "notes": [
          "Light scap/rotator cuff activation. Keep it submaximal."
        ]
      }
    }
  ],

  "module_notes": "General body warm-up. Use before primary modules. Specific finger warm-up belongs inside finger/hangboard modules."
}


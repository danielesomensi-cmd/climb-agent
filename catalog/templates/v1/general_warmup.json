{
  "id": "general_warmup",
  "name": "General Warm-up (Module)",
  "version": "1.1",
  "category": "warmup_module",
  "sports": ["climbing"],
  "disciplines": ["boulder", "lead", "mixed"],

  "compatible_slots": ["short", "medium", "long"],
  "requires_environment": { "equipment_any_of": ["none", "band", "mat", "floor"] },

  "goals": ["increase_body_temperature", "improve_joint_readiness", "activate_shoulders"],
  "stress_tags": { "fingers": "none", "cns": "low", "elbow": "low", "skin": "none" },

  "scheduling_rules": {
    "max_per_day": 2,
    "notes": "Use before any session. Skip if already warm (e.g., after jogging or easy climbing)."
  },

  "blocks": [
    {
      "block_id": "pulse_raise",
      "type": "warmup_general",
      "mode": "instruction_only",
      "duration_min_range": [3, 6],
      "options": ["brisk_walk", "easy_jog", "jumping_jacks", "air_squats_flow"],
      "notes": ["Raise body temperature slightly. Keep easy."]
    },
    {
      "block_id": "mobility",
      "type": "mobility",
      "mode": "instruction_only",
      "duration_min_range": [3, 6],
      "focus": ["thoracic_spine", "hips", "ankles"],
      "notes": ["Dynamic mobility only. No long static stretches."]
    },
    {
      "block_id": "upper_activation",
      "type": "activation",
      "mode": "select_one",
      "selection": {
        "primary": {
          "filters": {
            "required_tags_any": ["stim:shoulder_prehab", "stim:scapular_strength"],
            "exclude_tags_any": ["role:main", "protocol:max_hang", "stim:finger_max_strength"]
          }
        },
        "fallbacks": [
          {
            "filters": {
              "required_tags_any": ["stim:prehab", "stim:warmup_general"],
              "exclude_tags_any": ["role:main", "protocol:max_hang", "stim:finger_max_strength"]
            }
          }
        ]
      },
      "prescription": { "sets_range": [2, 3], "reps_range": [8, 15], "rest_seconds_range": [30, 60] },
      "notes": ["Light scap/rotator cuff activation. Submaximal."]
    }
  ],

  "module_notes": "General warm-up. Finger-specific warm-up belongs inside the finger module."
}
